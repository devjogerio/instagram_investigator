# Arquivo de exemplo para configuração de variáveis de ambiente
# Renomeie este arquivo para .env e preencha com suas informações

# Configurações gerais
DEBUG_MODE=False
CACHE_DURATION=3600  # Duração do cache em segundos (1 hora)

# Configurações do Instagram
INSTAGRAM_SESSION_ID=seu_session_id_aqui
INSTAGRAM_USERNAME=seu_usuario
INSTAGRAM_PASSWORD=sua_senha

# Configurações do Facebook
FACEBOOK_APP_ID=seu_app_id
FACEBOOK_APP_SECRET=seu_app_secret
FACEBOOK_ACCESS_TOKEN=seu_token_de_acesso

# Configurações do Twitter
TWITTER_API_KEY=sua_chave_api
TWITTER_API_SECRET=seu_segredo_api
TWITTER_ACCESS_TOKEN=seu_token_acesso
TWITTER_ACCESS_SECRET=seu_segredo_acesso

# Configurações do LinkedIn
LINKEDIN_CLIENT_ID=seu_client_id
LINKEDIN_CLIENT_SECRET=seu_client_secret
LINKEDIN_ACCESS_TOKEN=seu_token_acesso

# Configurações do TikTok
TIKTOK_CLIENT_KEY=sua_chave_cliente
TIKTOK_CLIENT_SECRET=seu_segredo_cliente
TIKTOK_ACCESS_TOKEN=seu_token_acesso